

<main style="z-index: 1;" class="h-svh z-10 flex flex-col pb-16  bg-blm">
  <nav class=" px-6 py-5 space-y-4  w-full  z-10 border-b border-b-blb ">
      <div class="flex  justify-between items-center">
      <h1 class="font-[500] text-black text-xl" ><%= navHead %></h1>
    </div>
    </nav>            

  <div id="quoteSection" class="h-full flex flex-col overflow-y-scroll  px-2 py-2 bg-blg gap-1 ">
    <div id="quotCustom" class="hidden flex h-full w-full flex-col justify-center items-center ">
      <div class="flex flex-col justify-center items-center mb-20  ">
       <img src="/bg/quotes.png"  alt="" class="w-32 h-32">
       <h1 class="font-[600] text-xl">No Posts</h1>
       <h1 class="text-blt ">All posts will appear here</h1>
      </div>
     </div>
     <% allQuotes.forEach(el => { %>
      <div class="quot ps-3  rounded-md bg-blm shadow-sm  border-b-2 border border-blb ">
          <div class="flex w-full flex-col py-3 px-2   " >
              <div class="flex  text-black gap-5 w-full ">
                 
                   <img src="/img/<%= el.wPic %>" alt="" class=" h-12 min-h-12 w-12 min-w-12 rounded-full ring-2  ">
               
          <div class=" bg- w-full ">
            <div class="flex flex-col justify-center gap-1 ">
              <div class="flex flex-col gap-4">
                <div>
                  <h1 class="text-black font-[500]" uid="<%= el.wId %>" ><%= el.wName %></h1>
                  <p id="quote" qId="<%= el.qId %>" class="">
                   <pre-wrap style="overflow-wrap: anywhere;" class="text-blt text-sm"><%= el.quote %></pre-wrap>
                 </p>
                </div>
              
              <div class="flex  justify-between items-center pt-3 pr-4 pb-1 w-full">
                                <div class=" flex items-center justify-center gap-1">
                                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-5">
                                  <path stroke-linecap="round" stroke-linejoin="round" d="M21 8.25c0-2.485-2.099-4.5-4.688-4.5-1.935 0-3.597 1.126-4.312 2.733-.715-1.607-2.377-2.733-4.313-2.733C5.1 3.75 3 5.765 3 8.25c0 7.22 9 12 9 12s9-4.78 9-12Z" />
                                </svg>
                                <p class="text-xs">20</p>
                                </div>
                               
                                <p class="text-xs">
                                  <%= el.time %>
                                 
                                </p>
                              </div>
              </div>
                                 
                                 
                                </div>


          </div>
                
               
               
               </div>
               
            </div>
          </div>
        
                  <% }) %>

  </div>
 

</main>
<%- include('./partials/footer.ejs') %>
<%- include('./partials/warningBox.ejs') %>


